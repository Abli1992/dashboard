{
  "name": "thc-dashboard-final",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "build:gh": "ng build --output-path=dist/thc-dashboard-final --base-href='/dashboard/'",
